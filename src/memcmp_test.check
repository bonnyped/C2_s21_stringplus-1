#suite memcmp_test

#test memcmp_test_1
  const char* t1 = "string_test num 1";
  const char* T1 = "string_test num 1";
  ck_assert((memcmp(t1, T1, 8) > 0 && s21_memcmp(t1, T1, 8) > 0) ||
            (memcmp(t1, T1, 8) < 0 && s21_memcmp(t1, T1, 8) < 0) ||
            (memcmp(t1, T1, 8) == 0 && s21_memcmp(t1, T1, 8) == 0)
  );

